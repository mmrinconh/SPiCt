	urldate = {2013-04-04},
	journal = {Deep Sea Research Part II: Topical Studies in Oceanography},
	author = {Catalán, I.A. and Rubín, J.P. and Navarro, G. and Prieto, L.},
	month = jun,
	year = {2006},
	keywords = {Engraulis encrasicolus, Ichthyoplankton, Larval assemblages, Physical oceanography, Transport processes, Wind-driven circulation},
	pages = {1377--1390},
	file = {ScienceDirect Full Text PDF:/home/marga/.mozilla/firefox/pj8fbkm5.default/zotero/storage/T9FV54AP/Catalán et al. - 2006 - Larval fish distribution in two different hydrogra.pdf:application/pdf;ScienceDirect Snapshot:/home/marga/.mozilla/firefox/pj8fbkm5.default/zotero/storage/K88V43Z5/S0967064506001123.html:text/html}
