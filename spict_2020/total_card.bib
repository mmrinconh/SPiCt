	pages = {1038--1044},
	file = {Full Text PDF:/home/marga/.mozilla/firefox/pj8fbkm5.default/zotero/storage/X8GKXGJC/Sethi et al. - 2012 - Managing harvest risk with catch-pooling cooperati.pdf:application/pdf;Snapshot:/home/marga/.mozilla/firefox/pj8fbkm5.default/zotero/storage/EGHSPCZP/1038.html:text/html}
