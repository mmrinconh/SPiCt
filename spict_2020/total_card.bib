	url = {http://www.sciencedirect.com/science/article/pii/S0165783699000107},
	doi = {10.1016/S0165-7836(99)00010-7},
	abstract = {Aspects of the reproductive biology of the Bay of Cadiz anchovy, Engraulis encrasicolus L., population have been studied from monthly samples taken from the Spanish purse-seine fishery during a four-year period (1989–1992). The spawning season extends from late winter to early autumn. The peak spawning time for the whole population occurs from June to August. However, larger anchovies (14–18.5 cm) are spring–summer spawners whereas anchovies in their first reproduction (11–13.5 cm) are mainly summer spawners. The sex ratio varies with size, females outnumbering males in the larger sizes. Annual sex ratios fulfill the theoretical 1:1 ratio, but vary seasonally. Maturity is attained at a total length of 11.09 cm in males and 11.20 cm in females. However, there are inter-annual variations in this size, suggesting a high plasticity in the reproductive characteristics as a response to the environmental factors regulating reproductive success. The seasonal cycle in anchovy condition appears to be related to both pulses in primary production and to the sexual cycle of the fish. Size-based differences in the energy investment in somatic growth and reproduction are suggested to explain the different reproductive patterns. The effects of a poor body condition in the anchovy ovary-weight cycle are discussed.},
	number = {1},
	urldate = {2015-12-02},
	journal = {Fisheries Research},
	author = {Millán, M},
	month = may,
	year = {1999},
	keywords = {anchovy, Bay of Cadiz, Condition, Eastern Atlantic, Engraulis encrasicolus, Reproductive biology},
	pages = {73--86},
	file = {ScienceDirect Snapshot:/home/marga/.mozilla/firefox/pj8fbkm5.default/zotero/storage/WQ5JNREJ/S0165783699000107.html:text/html}
}

