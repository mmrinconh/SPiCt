	month = aug,
	year = {2009},
	keywords = {Bayesian statistics, bioeconomics, decision analysis, stock–recruitment, uncertainty},
	pages = {fsp206},
	file = {Full Text PDF:/home/marga/.mozilla/firefox/pj8fbkm5.default/zotero/storage/EH3NGNJU/Mäntyniemi et al. - 2009 - The value of information in fisheries management .pdf:application/pdf;Snapshot:/home/marga/.mozilla/firefox/pj8fbkm5.default/zotero/storage/SSRE7BT5/icesjms.fsp206.html:text/html}
