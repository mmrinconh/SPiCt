	keywords = {anchovy, Engraulis encrasicolus, Fisheries oceanography, Gulf of Cádiz},
	pages = {1363--1376},
	file = {ScienceDirect Snapshot:/home/marga/.mozilla/firefox/pj8fbkm5.default/zotero/storage/XXBZW7H4/S0967064506001184.html:text/html}
}

