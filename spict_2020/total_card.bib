@article{burnham_kullback-leibler_2001,
	title = {Kullback-{Leibler} information as a basis for strong inference in ecological studies},
	volume = {28},
	url = {http://www.publish.csiro.au/paper/WR99107},
	abstract = {We describe an information-theoretic paradigm for analysis of ecological data, based on Kullback\&\#8211;Leibler information, that is an extension of likelihood theory and avoids the pitfalls of null hypothesis testing. Information-theoretic approaches emphasise a deliberate focus on the \textit{a priori} science in developing a set of multiple working hypotheses or models. Simple methods then allow these hypotheses (models) to be ranked from best to worst and scaled to reflect a strength of evidence using the likelihood of each model (\textit{g}$_{\textrm{i}}$), given the data and the models in the set (i.e. \textit{L}(\textit{g}$_{\textrm{i}}$ \&\#124; \textit{data})). In addition, a variance component due to model-selection uncertainty is included in estimates of precision. There are many cases where formal inference can be based on all the models in the \textit{a priori} set and this multi-model inference represents a powerful, new approach to valid inference. Finally, we strongly recommend inferences based on \textit{a priori} considerations be carefully separated from those resulting from some form of data dredging. An example is given for questions related to age- and sex-dependent rates of tag loss in elephant seals (\textit{Mirounga leonina}).},
	number = {2},
	urldate = {2013-01-30},
	journal = {Wildl. Res.},
	author = {Burnham, Kenneth P. and Anderson, David R.},
	month = jan,
	year = {2001},
	pages = {111--119},
	file = {CSIRO Publishing PDF:/home/marga/.mozilla/firefox/pj8fbkm5.default/zotero/storage/NM9EVKAF/Burnham and Anderson - 2001 - Kullback-Leibler information as a basis for strong.pdf:application/pdf}
}

